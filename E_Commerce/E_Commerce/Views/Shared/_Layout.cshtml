@model dynamic
@using E_Commerce.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
</head>
<body>
    <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="/Category/List">
                <img src="data:image/png;base64," alt="..." height="36">
            </a>

            <div class="collapse navbar-collapse" id="mynavbar">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                        <a asp-controller="Category" asp-action="List" class="nav-link">Categories</a>
                    </li>
                    <li class="nav-item">
                        <a asp-controller="Product" asp-action="List" class="nav-link">Products</a>
                    </li>
                    @if (!User.Identity.IsAuthenticated)
                    {
                        <li class="nav-item">
                            <a href="/Auth/Index" class="nav-link">SignIn</a>
                        </li>
                    }
                    else
                    {
                        <li class="nav-item">
                            <a asp-controller="Auth" asp-action="AdminLogout" class="nav-link">SignOut</a>
                        </li>
                    }

                </ul>
            </div>
        </div>
    </nav>

    @RenderBody()

</body>
</html>
